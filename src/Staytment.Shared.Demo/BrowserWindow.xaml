<Window x:Class="Staytment.Shared.Demo.BrowserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewModels="clr-namespace:Staytment.Shared.Demo.ViewModels"
        Title="{Binding Title}" Height="500" Width="600">
    <Window.DataContext>
        <viewModels:BrowserModel/>
    </Window.DataContext>
    <Grid>
        <WebBrowser Source="{Binding Url}" />
    </Grid>
</Window>
